{
  "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
  "name": "bare-template",
  "tags": [
    "scope:templates"
  ],
  "sourceRoot": "{projectRoot}/src",
  "projectType": "library",
  "targets": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "executor": "@nx/js:tsc",
      "options": {
        "main": "{projectRoot}/src/template.t.ts",
        "outputPath": "{projectRoot}/dist",
        "transformers": [
          "@dxos/log-hook/transformer"
        ],
        "tsConfig": "{projectRoot}/tsconfig.json"
      },
      "outputs": [
        "{options.outputPath}"
      ]
    },
    "lint": {},
    "test": {
      "dependsOn": [
        "build"
      ]
    }
  }
}
