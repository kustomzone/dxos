{
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "name": "composer-app",
  "tags": [
    "scope:apps"
  ],
  "sourceRoot": "{projectRoot}/src",
  "projectType": "library",
  "targets": {
    "build": {},
    "bundle": {
      "outputs": [
        "{projectRoot}/out/composer"
      ]
    },
    "e2e": {},
    "lint": {},
    "prebuild": {
      "executor": "nx:run-script",
      "options": {
        "script": "prebuild"
      },
      "outputs": [
        "{projcetRoot}/public/_worker.js",
        "{projectRoot}/public/assets/plugin-sketch"
      ]
    },
    "preview": {},
    "serve": {},
    "test": {}
  }
}
